{
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"printWidth": 80,
	"bracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"jsxBracketSameLine": false,
	"bracketSpacing": true,
	"proseWrap": "always",
	"object-property-newline": true,
	"object-curly-newline": {
		"multiline": true,
		"minProperties": 2
	},
	"plugins": ["prettier-plugin-tailwindcss"]
}
